import golosinas.*

test "tuttifrutti" {
	var tuttifrutti = new Tuttifrutti()
	tuttifrutti.libreGluten(true)
	assert.equals ("frutilla",tuttifrutti.gusto())
	tuttifrutti.mordisco()
	assert.equals ("chocolate", tuttifrutti.gusto())
	tuttifrutti.mordisco()
	assert.equals ("naranja", tuttifrutti.gusto())
	tuttifrutti.mordisco()
	assert.equals ("frutilla", tuttifrutti.gusto())
	

}